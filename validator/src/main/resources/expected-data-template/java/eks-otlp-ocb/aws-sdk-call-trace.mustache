    "aws.local.service": "^{{serviceName}}$",
    "aws.local.operation": "^GET /aws-sdk-call$",
    "aws.local.environment": "^generic:default$"
  },
  "metadata": {
      "EC2.AutoScalingGroup": "^eks-.+",
      "EKS.Cluster": "^{{platformInfo}}$",
      "K8s.Namespace": "^{{appNamespace}}",
